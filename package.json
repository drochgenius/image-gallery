{
    "name": "image-gallery-app",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "init": "mkdir -p dist/css; npm run sass",
        "test": "mocha-chrome test/runner.html --chrome-flags '[\"--headless\",\"--no-sandbox\",\"--disable-web-security\",\"--user-data-dir=/temp\"]'",
        "test-no-headless": "mocha-chrome test/runner.html --chrome-flags '[\"--no-sandbox\",\"--disable-web-security\",\"--user-data-dir=/temp\"]'",
        "intern-server": "intern serverOnly",
        "build": "tsc -p src",
        "watch-ts": "tsc -p src --watch",
        "watch-sass": "node-sass src/sass -r -o dist/css -w",
        "watch": "npm run init; concurrently --names ' ts ,sass' -c 'bgBlue,bgMagenta' 'npm run watch-ts' 'npm run watch-sass'",
        "sass": "node-sass src/sass -r -o dist/css",
        "dev": "polymer serve"
    },
    "keywords": [],
    "author": "Dominique Rochefort <dominique.rochefort@hmhco.com>",
    "license": "ISC",
    "devDependencies": {
        "@types/node": "^10.5.2",
        "chai": "^4.1.2",
        "concurrently": "latest",
        "mocha": "^5.2.0",
        "mocha-chrome": "^1.1.0",
        "node-sass": "^4.9.2",
        "polymer-cli": "latest",
        "typescript": "latest"
    },
    "dependencies": {
        "@material/button": "^0.37.1",
        "@polymer/lit-element": "latest",
        "lit-html": "latest"
    }
}
